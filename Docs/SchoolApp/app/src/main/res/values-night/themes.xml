        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/xiketic</item>
        <item name="colorPrimaryVariant">@color/blue_sapphire</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cyan_process</item>
        <item name="colorSecondaryVariant">@color/cyan_process</item>
        <item name="colorOnSecondary">@color/black</item>
